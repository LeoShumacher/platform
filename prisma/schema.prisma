generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int    @id @default(autoincrement())
  name         String
  email        String @unique
  password     String
  phone_number Int    @unique
  address      String
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  value       Int
  dob         DateTime
  dob_end     DateTime
}
